<?xml version="1.0" encoding="utf-8" ?>
<module>
    <id>VirtoCommerce.CatalogPublishing</id>
    <version>3.0.0</version>
    <platformVersion>3.0.0</platformVersion>
    <dependencies>
        <dependency id="VirtoCommerce.Catalog" version="3.0.0" />
        <dependency id="VirtoCommerce.Pricing" version="3.0.0" />
    </dependencies>

    <title>Catalog publishing module</title>
    <description>Allow you to see progress on filling in information about products and marks missed info.</description>
    <authors>
        <author>Alexey Antonov</author>
        <author>Andrew Orlov</author>
        <author>Aleksandr Vishnyakov</author>
        <author>Artem Dudarev</author>
    </authors>
    <owners>
        <owner>Virto Commerce</owner>
    </owners>
    <projectUrl>https://virtocommerce.com/apps/extensions/catalog-publishing</projectUrl>
    <iconUrl>https://github.com/VirtoCommerce/vc-module-catalog-publishing/raw/master/VirtoCommerce.CatalogPublishingModule.Web/Content/logo.png</iconUrl>
    <releaseNotes>Initial implementation of module features.</releaseNotes>
    <copyright>Copyright Â© 2018 Virto Commerce. All rights reserved</copyright>
    <tags>publishing filling</tags>

    <assemblyFile>VirtoCommerce.CatalogPublishingModule.Web.dll</assemblyFile>
    <moduleType>VirtoCommerce.CatalogPublishingModule.Web.Module, VirtoCommerce.CatalogPublishingModule.Web</moduleType>

    <styles>
        <directory virtualPath="$/Content" searchPattern="*.css" searchSubdirectories="true" />
    </styles>
    <scripts>
        <directory virtualPath="$/Scripts" searchPattern="*.js" searchSubdirectories="true" />
    </scripts>

    <permissions>
        <group name="CatalogPublishing">
            <permission id="channel:access" name="Open channels menu" description=""/>
            <permission id="channel:create" name="Create channel" description=""/>
            <permission id="channel:read" name="View channel" description=""/>
            <permission id="channel:update" name="Update channel" description=""/>
            <permission id="channel:delete" name="Delete channel" description=""/>
        </group>
    </permissions>
</module>
